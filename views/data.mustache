<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <ul class="todo-list">
            {{#users}}
              <li>
                <img src={{avatar}}>
                <h3><a>{{username}}</a></h3>
                {{#job}}
                <span>{{job}}, {{company}}</span>
                {{/job}}
                {{^job}}
                <span>Available for Hire</span>
                {{/job}}
              </li>
            {{/users}}
          </ul>
        </div>
      </div>
    </div>
  </body>
</html>
